{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "cover_image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "project_url": {
      "type": "string"
    },
    "repository_url": {
      "type": "string"
    },
    "published_date": {
      "type": "date"
    },
    "Desplegado": {
      "type": "boolean",
      "default": false
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technologie.technologie",
      "mappedBy": "projects"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::categorie.categorie",
      "mappedBy": "project"
    }
  }
}
